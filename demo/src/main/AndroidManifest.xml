<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.calstatela.jplone.demo">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity android:name=".MenuActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".BillboardCompassActivity" android:screenOrientation="portrait"/>
        <activity android:name=".BillboardLandmarksActivity" android:screenOrientation="portrait"/>
        <activity android:name=".CircleSceneActivity" android:screenOrientation="portrait"/>
        <activity android:name=".MountainDrawActivity" android:screenOrientation="portrait"/>
        <activity android:name=".ShapeDrawActivity" android:screenOrientation="portrait"/>
        <activity android:name=".GraphicsTestActivity" android:screenOrientation="portrait"/>


    </application>

</manifest>
